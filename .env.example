SOFTNIX_PROVIDER=openai
SOFTNIX_MODEL=gpt-4o-mini
SOFTNIX_MAX_ITERS=10
SOFTNIX_WORKSPACE=.
SOFTNIX_RUNS_DIR=.softnix/runs
SOFTNIX_SKILLS_DIR=skillpacks
SOFTNIX_SAFE_COMMANDS=ls,pwd,cat,echo,python,pytest,rm
SOFTNIX_EXEC_TIMEOUT_SEC=30
SOFTNIX_EXEC_RUNTIME=host
SOFTNIX_EXEC_CONTAINER_LIFECYCLE=per_action
SOFTNIX_EXEC_CONTAINER_IMAGE=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_PROFILE=auto
SOFTNIX_EXEC_CONTAINER_IMAGE_BASE=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_WEB=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_DATA=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_SCRAPING=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_ML=python:3.11-slim
SOFTNIX_EXEC_CONTAINER_IMAGE_QA=python:3.11-slim
# Recommended for stable data/ml tasks after build:
# SOFTNIX_EXEC_CONTAINER_IMAGE_DATA=softnix/runtime-data:py311
# SOFTNIX_EXEC_CONTAINER_IMAGE_ML=softnix/runtime-ml:py311
SOFTNIX_EXEC_CONTAINER_NETWORK=none
SOFTNIX_EXEC_CONTAINER_CPUS=1.0
SOFTNIX_EXEC_CONTAINER_MEMORY=512m
SOFTNIX_EXEC_CONTAINER_PIDS_LIMIT=256
SOFTNIX_EXEC_CONTAINER_CACHE_DIR=.softnix/container-cache
SOFTNIX_EXEC_CONTAINER_PIP_CACHE_ENABLED=true
SOFTNIX_MAX_ACTION_OUTPUT_CHARS=12000
SOFTNIX_NO_PROGRESS_REPEAT_THRESHOLD=3
SOFTNIX_WEB_FETCH_TLS_VERIFY=true
SOFTNIX_MEMORY_PROFILE_FILE=memory/PROFILE.md
SOFTNIX_MEMORY_SESSION_FILE=memory/SESSION.md
SOFTNIX_MEMORY_POLICY_PATH=.softnix/system/POLICY.md
SOFTNIX_MEMORY_PROMPT_MAX_ITEMS=20
SOFTNIX_MEMORY_INFERRED_MIN_CONFIDENCE=0.75
SOFTNIX_MEMORY_PENDING_ALERT_THRESHOLD=10
SOFTNIX_MEMORY_ADMIN_KEY=
SOFTNIX_TELEGRAM_ENABLED=false
SOFTNIX_TELEGRAM_MODE=polling
SOFTNIX_TELEGRAM_BOT_TOKEN=
SOFTNIX_TELEGRAM_ALLOWED_CHAT_IDS=
SOFTNIX_TELEGRAM_WEBHOOK_SECRET=
SOFTNIX_TELEGRAM_POLL_INTERVAL_SEC=1.0
SOFTNIX_TELEGRAM_MAX_TASK_CHARS=2000
SOFTNIX_API_KEY=
SOFTNIX_CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
SOFTNIX_CORS_ALLOW_CREDENTIALS=true
SOFTNIX_OPENAI_API_KEY=
SOFTNIX_OPENAI_BASE_URL=https://api.openai.com/v1
SOFTNIX_CLAUDE_API_KEY=
SOFTNIX_CLAUDE_BASE_URL=https://api.anthropic.com
SOFTNIX_CUSTOM_API_KEY=
SOFTNIX_CUSTOM_BASE_URL=
SOFTNIX_CUSTOM_MODEL=gpt-4o-mini
